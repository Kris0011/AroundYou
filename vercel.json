{
    "version" : 2 ,
    "builds" : [
        {
            "src" : "AroundYou/wsgi.py",
            "use" : "@vercel/python",
            "config" : { "maxLambdaSize" : "15mb"}

        } ,
        {
            "src" : "build.sh",
            "use" : "@vercel/static-build",
            "config" : { "distDir" : "staticfiles_build" }
        }
    ],
    "routes" : [
        {
            "src" : "/static/(.*)",
            "dest" : "/static/$1"
        } ,
        {
            "src" : "/(.*)",
            "dest" : "AroundYou/wsgi.py"
        }
    ]
}